<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor default="markup-element" foreColor="ffffcccc" name="xml-tag"/>
    <fontcolor default="default" foreColor="ffffcccc" name="xml-value">
        <font style="italic"/>
    </fontcolor>
    <fontcolor default="default" foreColor="ffff0099" name="xml-doctype">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="default" foreColor="ffcc99ff" name="xml-text"/>
    <fontcolor default="default" foreColor="ffcc99ff" name="xml-cdata-section">
        <font style="italic"/>
    </fontcolor>
    <fontcolor default="operator" foreColor="ffff0099" name="xml-operator"/>
    <fontcolor default="default" name="xml-pi-end">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="default" name="xml-pi-target">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="default" name="xml-pi-start">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="markup-attribute" foreColor="ffff0099" name="xml-attribute">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="default" name="xml-pi-content"/>
</fontscolors>
