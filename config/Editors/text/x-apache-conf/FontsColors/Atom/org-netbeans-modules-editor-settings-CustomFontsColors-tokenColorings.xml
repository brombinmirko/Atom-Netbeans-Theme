<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor default="keyword" foreColor="ffff99cc" name="tagparam">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="separator" foreColor="ff99ff33" name="flag">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="error" name="error"/>
    <fontcolor default="keyword" foreColor="ffffcc99" name="tag"/>
    <fontcolor default="string" name="string"/>
    <fontcolor default="whitespace" name="whitespace"/>
    <fontcolor default="number" name="number"/>
    <fontcolor default="keyword" name="directive"/>
    <fontcolor default="comment" name="comment"/>
    <fontcolor default="identifier" foreColor="ffff0099" name="variable">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="keyword" foreColor="white" name="directiveparam"/>
</fontscolors>
